<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net462;netcoreapp3.0</TargetFrameworks>
    <Version>1.0.3</Version>
    <Authors>AlexJagello</Authors>
    <Company>AlexJagello</Company>
    <Description>Lib for numerical solve of first order differential eqation </Description>
    <AssemblyVersion>1.0.2.3</AssemblyVersion>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/AlexJagello/DifferentialEquationNumericalSolver</PackageProjectUrl>
